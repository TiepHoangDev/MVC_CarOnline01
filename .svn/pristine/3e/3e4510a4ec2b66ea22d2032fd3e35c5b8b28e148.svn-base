<script>
    $('.short-link').on('click', function () {
        if ($('.short-link').hasClass('share-link')) {
            $('.short-link').removeClass('share-link');
            $('.share-tip').css('display', 'none');
        } else {
            $('.short-link').addClass('share-link');
            $('.share-tip').css('display', 'inline-block');
        }

    });

    $('.close').on('click', function () {

        $('.short-link').removeClass('share-link');
        $('.share-tip').css('display', 'none');
    });

</script>