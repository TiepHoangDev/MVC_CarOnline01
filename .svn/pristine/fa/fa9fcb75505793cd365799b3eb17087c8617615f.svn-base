            <script>
                $(function () {
                    $('.rootcategory-right').width($('.rootcategory-menu').width() - $('.nearby-button').width() - $('.dropdown1').width() - 630);
                    $("#btnInviteFriend, #btnInviteFriendFooter").live("click", function () {
                        CommonHelper.InviteFriend('/moi-tham-gia/tu-email');
                    });
                    if ($("#pkeywords").val().length == 0) {
                        $('#pkeywords').addClass('watermark');
                    } else {
                        $('#pkeywords').removeClass('watermark');
                    }
                    $("#pkeywords").watermark('Địa điểm, món ăn, loại hình...', {
                        useNative: true,
                        className: 'watermark'
                    })
                    .focus(function () {
                        if ($(this).val().length == 0)
                            $(this).addClass('watermark');
                        else
                            $(this).removeClass('watermark');
                    })
                    .keyup(function () {
                        if ($(this).val().length == 0)
                            $(this).addClass('watermark');
                        else
                            $(this).removeClass('watermark');
                    })
                    .blur(function () {
                        if ($(this).val().length == 0)
                            $(this).addClass('watermark');
                        else
                            $(this).removeClass('watermark');
                    });
                })
            </script><div class="container-topbar">
