}

    .room-product-info-contact span {
        color: #5f8dc2;
    }

.room-product-info-title > a {
    color: #000;
    line-height: 16px;
    text-decoration: none;
    font: normal 14px/18px Arial;
}

.room-product-info-phone {
    overflow: hidden;
    padding: 2px 0;
    white-space: nowrap;
}

.room-product-info-area {
    overflow: hidden;
    white-space: nowrap;
}

.chat-caller-bds .support-title {
    display: block;
    font-weight: bold;
    width: 122px;
    word-wrap: break-word;
    padding-top: 11px;
}

.chat-caller-bds .support-like, .chat-caller-bds .support-dislike {
    width: 16px;
    height: 17px;
    display: inline-block;
}

.chat-caller-bds .seprate-like {
    background-color: #d8dae0;
    display: inline-block;
    width: 1px;
    height: 17px;
    margin: 0px 3px;
}

.chat-caller-bds .support-like {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-like-icon.png");
}

.chat-caller-bds .support-dislike {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-dislike-icon.png");
}

/* welcome message */
.chat-welcome-message .welcome-img {
    float: left;
    width: 32px;
}

.chat-welcome-message {
    padding: 2px;
}

    .chat-welcome-message .welcome-img img {
        width: 32px;
        height: 32px;
    }

    .chat-welcome-message .welcome-message {
        border: 2px solid #c2c2c2;
        margin-left: 5px;
        padding: 2px;
        text-align: center;
        width: auto;
        margin-left: 35px;
    }
/* Room setting */

.chatPopup-header .visible {
    display: block;
}

.room-setting {
    display: none;
    position: absolute;
    top: 100%;
    box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    -moz-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    -webkit-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    right: 0;
    width: 214px;
    background-color: #fff;
    color: #1c1c1c;
    font-weight: normal;
    z-index: 1;
    left: 197px;
}

.room-setting-content {
    border: 1px solid #c3c3c3;
    border-width: 0px 1px 1px 1px;
    padding-left: 8px;
    padding-bottom: 10px;
}

.room-setting div.setting-title {
    text-align: center;
    font-weight: bold;
    background-color: #004680;
    color: white;
    position: relative;
}

.setting-colse {
    position: absolute;
    color: #fff;
    display: inline-block;
    right: 2px;
    height: 100%;
    line-height: 27px;
    width: 20px;
    text-decoration: none;
    background-position: 5px center;
    background-repeat: no-repeat;
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-close-icon.png");
    cursor: pointer;
}

.divIsRoomSound, .divIsSendEmail {
    background: url("https://img1.banxehoi.com/chat_images/chats/chat-checked-icon.png") no-repeat scroll 0px center rgba(0, 0, 0, 0);
    cursor: pointer;
    padding-left: 18px;
}

.divTxtSendEmail {
    border-bottom: 1px solid #c3c3c3;
    padding: 0px 0px 10px 0px;
    width: 198px;
}

    .divTxtSendEmail input, .setting-email input {
        color: #004680;
    }

.edit-user-info {
    color: #004680;
    font-weight: bold;
}

.room-setting-content input {
    border: 1px solid #c3c3c3;
    height: 19px;
    padding-left: 8px;
    width: 187px;
}

.setting-spam a {
    color: #515151;
    display: inline-block;
    line-height: 20px;
    text-decoration: underline;
}

.btnSetting {
    width: 90px;
    height: 24px;
    line-height: 24px;
    background-color: #0464b4;
    font-weight: bold;
    display: inline-block;
    text-align: center;
    text-decoration: none;
    color: #fff;
    border-radius: 8px;
    -moz-border-radius: 8px;
    -webkit-border-radius: 8px;
}

    .btnSetting.btnclear {
        background-color: #ededed;
        color: #0464b4;
        margin-left: 7px;
    }

        .btnSetting.btnclear:active {
            color: #fff;
        }

    .btnSetting.btnsave {
        margin-left: 3px;
    }

/* Error message */
.chatRoom .chat-message-error {
    /*background-color: white;*/
    background-position: 2px center;
    border-top: 1px solid #aaa;
    color: red;
}

    .chatRoom .chat-message-error .error-icon {
        background-image: url('https://img1.banxehoi.com/chat_images/chats/error_icon.png');
        background-repeat: no-repeat;
        height: 32px;
        width: 32px;
    }

.chat-message-error > table {
    width: 100%;
}


