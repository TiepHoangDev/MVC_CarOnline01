.chat-user-mgr-chat-area .chatRoom .room-product-info, .chatRoom .chat-caller-bds {
    padding-right: 5px;
    padding-top: 10px;
    height: 89px;
    background: #ddf3ff;
    padding-left: 10px;
    float: left;
    width: 420px;
}

.chatPopup .chat-user-mgr-chat-area .chatRoom .room-product-info, .chatRoom .chat-caller-bds {
    padding-right: 5px;
    padding-top: 10px;
    height: 89px;
