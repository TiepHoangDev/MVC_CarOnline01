    .chatRoom .room-product-info .hr,
    .chatRoom .chat-caller-bds .hr {
        /*background-color: #aaa;*/
        background: none repeat scroll 0 0 #d8dae0;
        height: 1px;
        margin: 5px auto 0;
        width: 100%;
    }


/* Room product Info */
.room-product-info-img {
    float: left;
    height: 67px;
    width: 90px;
}

    .room-product-info-img img {
        height: 72px;
        width: 75px;
    }

.room-product-info-content {
    float: left;
    margin-top: -3px;
    width: 135px;
}

.room-product-info-title {
    /* font-weight: bold; */
    max-height: 57px;
    overflow: hidden;
    margin-bottom: 3px;
}

.room-product-info-contact {
    text-align: left;
    /* font-weight: bold; */
