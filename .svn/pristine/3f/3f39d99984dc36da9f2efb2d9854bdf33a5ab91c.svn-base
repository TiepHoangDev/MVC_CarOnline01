/* Chat bds */
.chat-header-bds {
    /*border-radius: 8px;*/
    border-top-right-radius: 8px;
    border-top-left-radius: 8px;
    /*-moz-border-radius: 8px;
    -webkit-border-radius: 8px;*/
    height: 35px;
    width: 252px;
}

.link-chat-bds {
    line-height: 35px;
    display: inline-block;
    width: 100%;
}

.link-chat-bds-header {
    height: 35px;
    float: left;
    width: 42px;
    border-radius: 8px 0 0 8px;
    -moz-border-radius: 8px 0 0 8px;
    -webkit-border-radius: 8px 0 0 8px;
    background: url("images/chat-comment-icon.png") no-repeat scroll center center #ffffff;
    display: block;
    background-position: 10px 12px;
}

.link-chat-bds-header-detail {
    height: 35px;
    float: left;
    width: 35px;
    border-radius: 8px 0 0 8px;
    -moz-border-radius: 8px 0 0 8px;
    -webkit-border-radius: 8px 0 0 8px;
    background: url("images/chat-comment-icon2.png") no-repeat scroll;
    display: block;
    background-position: 6px 6px;
}


.chat-with-bds {
    background-color: #135eac;
    box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    -moz-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    -webkit-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    margin-right: 10px;
}

    .chat-with-bds .link-chat-bds-header {
        background-color: #135eac;
        /* border-right: 1px solid #65b66d; */
        margin: 0 5px;
    }

.chat-with-product-owner {
    background-color: #055699;
}

    .chat-with-product-owner .link-chat-bds-header {
        background-color: #0464b4;
        border-right: 1px solid #004680;
    }

.link-chat-bds-content {
    color: #ffffff;
