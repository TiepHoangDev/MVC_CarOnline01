/*Note: Style have use css3, To fix css3 on ie then using css3Pie: http://css3pie.com/ */
.chat-container,
.chat-container a,
.chat-container ul,
.chat-container li,
.chat-container table,
.chat-container tr,
.chat-container td {
    font-family: 'Open sans' !important;
    font-size: 12px;
}

.chat-container {
    display: block;
    position: fixed;
    bottom: 0;
    right: 0;
    z-index: 9999;
    background-color: #fff;
}

.chatRooms {
    max-height: 500px;
    overflow: auto;
    color: #3e454c;
}

.chatRoom {
    margin: 0px;
    border: 1px solid #c3c3c3;
    border-width: 0px 1px 0px 1px;
    padding: 0;
    background-color: #fff;
}

.chatMessages {
    width: 96%;
    height: 200px;
    overflow: auto;
    margin-left: 0px;
    padding-left: 10px;
}

    .chatMessages li {
        list-style-type: none;
        padding: 1px;
    }

.chatNewMessage {
    border: 0 none;
    display: block;
    font-family: tahoma;
    font-size: 12px;
    line-height: 22px;
    min-height: 35px;
    outline: medium none;
    overflow: hidden;
    padding-right: 0;
    resize: none;
    white-space: pre-wrap;
    width: 218px;
    height: 35px;
    padding: 0 !important;
    font-style: normal;
}

.div-emoticon {
    position: absolute;
    right: 9px;
    bottom: 7px;
}

.chatMessage {
    margin-top: 0px;
}

.chatSend {
    margin-top: 0px;
    float: left;
    display: none;
}

.emoticon-avatar {
    float: right;
    padding-right: 2px;
}

.chat-command span.emoticon-avatar {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-emoticon-icon.png") !important;
    width: 14px !important;
    height: 14px !important;
}

.clear {
    clear: both;
}

.sendmessage-container {
    /* padding-bottom: 5px; */
    /* padding-left: 10px; */
}

.chat-command {
    border-top: 1px solid #1260ab;
    background-color: #fff;
    /* margin-right: 15px; */
    margin-top: 10px;
    height: 40px;
    position: relative;
    display: block;
    padding-left: 7px;
}
/*Message box*/
.chatMessages div.message {
    background-color: #fff;
    border: none;
    margin: 10px 0 0;
    padding: 0;
}

    .chatMessages div.message .clear {
        clear: both;
    }

.chatMessages div.left {
    float: left;
    position: relative;
    min-height: 32px;
    /*text-align: justify;*/
}

.chatMessages div.right {
    /*float: right;*/
    /*text-align: justify;*/
}

.chatMessages div.left .avatar {
    width: 32px;
    height: 32px;
    position: absolute;
    left: 2px;
    /*border: 1px solid #c2c2c2;
    border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;*/
}

    .chatMessages div.left .avatar img {
        border-radius: 25px;
        -moz-border-radius: 25px;
        -webkit-border-radius: 25px;
        /* border: 1px solid #cfcfcf; */
        height: 32px;
        width: 32px;
        margin-top: 4px;
    }


.chatMessages div.left .msg-text-item {
    padding: 0px;
    margin-left: 33px;
}

.chatMessages .msg-text-item .bounds {
    max-width: 165px;
    overflow: hidden;
}

    .chatMessages .msg-text-item .bounds .text-message {
        border: 1px solid #cfcfcf;
        border-radius: 5px;
        -moz-border-radius: 5px;
        -webkit-border-radius: 5px;
        margin-top: 3px;
        padding: 5px;
        position: relative;
        word-wrap: break-word;
    }

        .chatMessages .msg-text-item .bounds .text-message span {
            font: normal 13px Arial !important;
        }

        .chatMessages .msg-text-item .bounds .text-message a {
            color: #055699;
        }

.chatMessages .left .msg-text-item .bounds {
    margin-left: 2px;
    padding-left: 12px;
}

.chatMessages .right .msg-text-item .bounds {
    margin-right: 2px;
    padding-right: 12px;
    max-width: 215px;
}

/*.chatMessages .left .text-message, .chatMessages .left .text-message:after, .chatMessages .left .text-message:before
{
    background-color: #fff;
}
.chatMessages .right .text-message, .chatMessages .right .text-message:after, .chatMessages .right .text-message:before 
{
    background-color: #fff;
}*/

.chatMessages .left .text-message {
    background-color: #ffffff;
    color: #515151;
}

.chatMessages .right .text-message {
    background-color: #ededed;
    color: #565656;
}


.chatMessages .text-message:after, .chatMessages .text-message:before {
    content: "";
    position: absolute;
    width: 10px;
}

.chatMessages .text-message:after {
    /*background-image: url("https://img1.banxehoi.com/chat_images/chats/facebook-icon.png");*/
    background-repeat: no-repeat;
    background-size: auto;
    top: 8px;
    height: 10px;
}

.chatMessages .left .text-message:before, .chatMessages .right .text-message:before {
    top: 0;
    bottom: 0;
    clip: rect(10px, 13px, 18px, 0);
}

.chatMessages .left .text-message:before, .chatMessages .left .text-message:after {
    right: 100%;
}

.chatMessages .right .text-message:before, .chatMessages .right .text-message:after {
    left: 100%;
}

.chatMessages .left .text-message:after {
    /*background-position: -302px -272px;*/
    background: url("https://img1.banxehoi.com/chat_images/chats/chat-arrow-left-icon.png") no-repeat;
}

.chatMessages .right .text-message:after {
    background: url("https://img1.banxehoi.com/chat_images/chats/chat-arrow-right-icon.png") no-repeat;
}

.chatMessages .chat-time {
    background-color: #d8dae0;
    color: #5a5a5a;
    display: block;
    font-size: 10px;
    font-weight: bold;
    height: 1px;
    margin: 16px 0 15px;
    padding: 0;
    text-align: right;
}

    .chatMessages .chat-time span {
        font-size: 10px;
    }

    .chatMessages .chat-time div {
        position: relative;
        top: -9px;
    }

.chat-time span {
    background: url("https://img1.banxehoi.com/chat_images/chats/chat-clock-icon.png") no-repeat scroll 7px center #fff;
    padding: 0 5px 0 20px;
}

.chat-hiddendiv {
    overflow: hidden;
    margin: 0px;
    padding: 0px;
    display: none;
    white-space: pre-wrap;
    word-wrap: break-word;
    overflow: hidden;
    min-height: 35px;
    max-height: 180px;
    width: 180px;
    font-size: 12px;
    font-family: tahoma;
    line-height: 18px;
}

.lbr {
    line-height: 5px;
}

/*Chat popup*/
.chatPopup-header {
    background-color: #135eac;
    border-radius: 5px 5px 0 0;
    -moz-border-radius: 5px 5px 0 0;
    -webkit-border-radius: 5px 5px 0 0;
    color: #fff;
    /* font-weight: bold; */
    height: 41px;
    line-height: 35px;
    margin: 0;
    padding: 0 5px 0 10px;
    position: relative;
}

.chatPopup .header-blink {
    background-color: #3d9e47;
}

    .chatPopup .header-blink .chat-header-bds {
        background-color: #0464b4;
    }

/*.header-blink .header-icon a
{
	border: 1px solid #00bfff !important;
}*/

.chatPopup-header .header-title {
    float: left;
    white-space: nowrap;
    font-family: 'Roboto Bold';
    font-weight: bold;
    font-size: 16px;
    line-height: 40px;
}

    .chatPopup-header .header-title .unread-count {
        color: #ff8c00;
    }

.header-icon {
    height: 100%;
    position: absolute;
    right: 3px;
    top: 3px;
}

    .header-icon a {
        color: #fff;
        display: inline-block;
        float: right;
        height: 89%;
        line-height: 27px;
        width: 20px;
        text-decoration: none;
        background-position: 5px center;
        background-repeat: no-repeat;
    }

.chatPopup-header .header-icon a.header-icon-minimize {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-minimize-icon.png");
}

.chatPopup-header .header-icon a.header-icon-information {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-information-icon.png");
    padding-right: 2px;
}

.chatPopup-header-maximinze .header-icon a.header-icon-minimize,
.collapse-chat-room.minimize .header-icon a.header-icon-minimize,
.chat-reg.minimize .header-icon a.header-icon-minimize {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-maximize-icon.png");
    background-position: 4px center;
}

.chatPopup-header-maximinze .header-icon a.header-icon-setting {
    display: none;
}

.chatPopup-header-maximinze .chatRoom {
    display: none;
}

.chatPopup-header:hover {
    /*cursor: pointer;*/
}

.header-icon a.header-icon-close {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-close-icon.png");
}

.header-icon a.header-icon-minimize {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-minimize-icon.png");
}

.chatPopup-header .header-icon a.header-icon-setting {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-setting-icon.png");
}

    .chatPopup-header .header-icon a.header-icon-setting span {
        /*background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-minimize-icon.png");*/
    }

.header-icon a:hover, .chatPopup-header .header-icon a.hover {
    background-color: #004680;
}

.chatPopup {
    display: none;
    width: 250px;
    /*border: 1px solid #c3c3c3;*/
    border-radius: 8px 8px 0 0;
    -moz-border-radius: 8px 8px 0 0;
    -webkit-border-radius: 8px 8px 0 0;
    font-size: 13px;
    font-family: Arial;
    bottom: 0;
    box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    -moz-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    -webkit-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);
    background-color: #fff;
    z-index: 9999;
}

    .chatPopup.show {
        display: block;
    }

.online-status {
    background-image: url("/Content/Chat/styles/images/chat_online_icon2.png") !important;
}

.user-status {
    background-image: url("https://img1.banxehoi.com/chat_images/chats/chat-offline-icon.png");
    background-position: left center;
    background-repeat: no-repeat;
    padding: 0 10px 0 7px;
}

